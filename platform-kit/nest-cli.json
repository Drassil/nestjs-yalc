{
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "compilerOptions": {
    "webpack": true,
    "webpackConfigPath": "webpack.config.cjs",
    "tsConfigPath": "!!!!!!!!!!!please-specify-a-valid-project-name!!!!!!!!!!!"
  },
  "projects": {
    "user-module": {
      "type": "library",
      "root": "libs/user-module",
      "entryFile": "index",
      "sourceRoot": "libs/user-module/src",
      "compilerOptions": {
        "tsConfigPath": "libs/user-module/tsconfig.lib.json"
      }
    },
    "platform": {
      "type": "application",
      "root": "apps/_srv/platform",
      "entryFile": "main",
      "sourceRoot": "apps/_srv/platform/src",
      "compilerOptions": {
        "tsConfigPath": "apps/_srv/platform/tsconfig.app.json"
      }
    }
  }
}
