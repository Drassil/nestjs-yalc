{
  "name": "@nestjs-yalc/platform-kit",
  "custom": {
    "nest": {
      "type": "project"
    }
  },
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "nest build",
    "build:watch": "nest build --watch",
    "lint": "npm run lint:no-fix -- --fix",
    "lint:no-fix": "eslint -c .eslintrc.cjs \"**/src/**/*.ts\" --max-warnings=0",
    "lint:path": "eslint -c .eslintrc.cjs $npm_config_lintpath\"/**/*.ts\" --max-warnings=0",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules --experimental-import-meta-resolve\" jest ",
    "test:watch": "npm run test -- --watch",
    "test:cov": "npm run test -- --coverage",
    "test:cov:serve": "npx http-server -c-1 ./docs/compodoc/jestcoverage/",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/sk-app/test/jest-e2e.json",
    "npm:update:onlylock": "npm update --package-lock-only",
    "npm:outdated": "npm outdated",
    "ncu:interactive": "npx -y npm-check-updates -i",
    "ncu": "npx -y npm-check-updates -u",
    "prebuild": "rimraf dist",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/sk-app/main",
    "update:lock": "npm install --package-lock-only --workspaces false"
  },
  "dependencies": {
    "@apollo/gateway": "^2.7.1",
    "@nestjs-yalc/api-strategy": "../api-strategy",
    "@nestjs-yalc/app": "../app",
    "@nestjs-yalc/aws-helpers": "../aws-helpers",
    "@nestjs-yalc/crud-gen": "../crud-gen",
    "@nestjs-yalc/data-loader": "../data-loader",
    "@nestjs-yalc/database": "../database",
    "@nestjs-yalc/errors": "../errors",
    "@nestjs-yalc/event-manager": "../event-manager",
    "@nestjs-yalc/field-middleware": "../field-middleware",
    "@nestjs-yalc/graphql": "../graphql",
    "@nestjs-yalc/interfaces": "../interfaces",
    "@nestjs-yalc/jest": "../jest",
    "@nestjs-yalc/logger": "../logger",
    "@nestjs-yalc/pk-app-user": "file:./apps/user",
    "@nestjs-yalc/platform-modules": "../platform-modules",
    "@nestjs-yalc/scripts": "../scripts",
    "@nestjs-yalc/toolkit-config": "../toolkit-config",
    "@nestjs-yalc/types": "../types",
    "@nestjs-yalc/utils": "../utils",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/common": "^10.3.1",
    "@nestjs/core": "^10.3.1",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/platform-express": "^10.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0",
    "ts-morph": "^21.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/testing": "^10.3.1",
    "@types/express": "^4.17.16",
    "@types/jest": "29.2.6",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.0",
    "prettier": "^2.8.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.3.3",
    "webpack-visualizer-plugin2": "^1.1.0"
  }
}
