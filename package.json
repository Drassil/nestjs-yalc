    "@jest/globals": "^29.4.2",
    "@swc/core": "^1.3.35",
    "@swc/helpers": "^0.4.14",
    "@types/cli-color": "^2.0.2",
    "@types/lodash": "^4.14.191",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esm": "^3.2.25",
    "get-tsconfig": "^4.4.0",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
