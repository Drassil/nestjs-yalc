{
  "name": "@nestjs-yalc/framework",
  "type": "module",
  "version": "1.3.2",
  "description": "NestJS - Yet Another Library Collection",
  "scripts": {
    "build": "tsc --noEmit",
    "build:watch": "tsc-watch --noEmit",
    "lint": "npm run lint:no-fix -- --fix",
    "lint:no-fix": "eslint -c .eslintrc.cjs \"**/src/**/*.ts\" --max-warnings=0",
    "lint:path": "eslint -c .eslintrc.cjs $npm_config_lintpath\"/**/*.ts\" --max-warnings=0",
    "test": "node --experimental-vm-modules --experimental-import-meta-resolve ./node_modules/.bin/jest ",
    "test:watch": "npm run test -- --watch",
    "test:cov": "npm run test -- --coverage",
    "test:cov:serve": "npx http-server -c-1 ./docs/compodoc/jestcoverage/",
    "test:vscode": "node test.js ",
    "npm:update:onlylock": "npm update --package-lock-only",
    "npm:outdated": "npm outdated",
    "ncu:interactive": "npx -y npm-check-updates -i",
    "ncu": "npx -y npm-check-updates -u"
  },
  "author": "",
  "license": "MIT",
  "workspaces": [
    "app",
    "crud-gen",
    "aws-helpers",
    "data-loader",
    "database",
    "errors",
    "field-middleware",
    "graphql",
    "interfaces",
    "jest",
    "jest/src/config",
    "logger",
    "types",
    "starter-kit",
    "utils"
  ],
  "peerDependencies": {
    "@nestjs/apollo": "^10.2.0",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-fastify": "^9.2.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1"
  },
  "dependencies": {
    "@apollo/federation": "^0.38.1",
    "@apollo/subgraph": "^2.3.2",
    "@fastify/cookie": "^8.3.0",
    "@jorgebodega/typeorm-seeding": "^6.0.1",
    "@types/node-fetch": "^2.6.2",
    "apollo-server-fastify": "^3.4.0",
    "aws-lambda-fastify": "^2.2.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.0",
    "commander": "^10.0.0",
    "csurf": "^1.11.0",
    "dataloader": "^2.2.1",
    "decimal.js": "^10.4.3",
    "faker": "^6.6.6",
    "google-libphonenumber": "^3.2.32",
    "lodash": "^4.17.21",
    "mobile-detect": "^1.4.5",
    "mysql2": "^3.1.2",
    "node-fetch": "^3.3.0",
    "p-map": "^5.5.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.12",
    "typeorm-model-generator": "^0.4.6",
    "uuidv4": "^6.2.13"
