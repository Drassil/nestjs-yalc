{
  "compilerOptions": {
    "strict": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "strictBindCallApply": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "strictPropertyInitialization": false,
    "useDefineForClassFields": false,
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "var/dist",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "allowJs": true,
    "sourceMap": true,
    "baseUrl": "./",
    "incremental": true,
    "importHelpers": true,
    "isolatedModules": true,
    "preserveConstEnums": true,
    "noEmit": true,
    "paths": {
      "@nestjs-yalc/api-strategy": [
        "api-strategy/src"
      ],
      "@nestjs-yalc/api-strategy/*": [
        "api-strategy/src/*"
      ],
      "@nestjs-yalc/app": [
        "app/src"
      ],
      "@nestjs-yalc/app/*": [
        "app/src/*"
      ],
      "@nestjs-yalc/crud-gen": [
        "crud-gen/src"
      ],
      "@nestjs-yalc/crud-gen/*": [
        "crud-gen/src/*"
      ],
      "@nestjs-yalc/database": [
        "database/src"
      ],
      "@nestjs-yalc/database/*": [
        "database/src/*"
      ],
      "@nestjs-yalc/aws-helpers": [
        "aws-helpers/src"
      ],
      "@nestjs-yalc/aws-helpers/*": [
        "aws-helpers/src/*"
      ],
      "@nestjs-yalc/data-loader": [
        "data-loader/src"
      ],
      "@nestjs-yalc/data-loader/*": [
        "data-loader/src/*"
      ],
      "@nestjs-yalc/errors": [
        "errors/src"
      ],
      "@nestjs-yalc/errors/*": [
        "errors/src/*"
      ],
      "@nestjs-yalc/field-middleware": [
        "field-middleware/src"
      ],
      "@nestjs-yalc/field-middleware/*": [
        "field-middleware/src/*"
      ],
      "@nestjs-yalc/graphql": [
        "graphql/src"
      ],
      "@nestjs-yalc/graphql/*": [
        "graphql/src/*"
      ],
      "@nestjs-yalc/interfaces": [
        "interfaces/src"
      ],
      "@nestjs-yalc/interfaces/*": [
        "interfaces/src/*"
      ],
      "@nestjs-yalc/logger": [
        "logger/src"
      ],
      "@nestjs-yalc/logger/*": [
        "logger/src/*"
      ],
      "@nestjs-yalc/utils": [
        "utils/src"
      ],
      "@nestjs-yalc/utils/*": [
        "utils/src/*"
      ],
      "@nestjs-yalc/jest": [
        "jest/src"
      ],
      "@nestjs-yalc/jest/*": [
        "jest/src/*"
      ],
      "@nestjs-yalc/types": [
        "types/src"
      ],
      "@nestjs-yalc/types/*": [
        "types/src/*"
      ],
      "@nestjs-yalc/kafka": [
        "kafka/src"
      ],
      "@nestjs-yalc/kafka/*": [
        "kafka/src/*"
      ],
      "@nestjs-yalc/skeleton-module": [
        "examples/skeleton-module/src"
      ],
      "@nestjs-yalc/skeleton-module/*": [
        "examples/skeleton-module/src/*"
      ]
    }
  },
  "include": [
    "**/src/**/*.ts"
  ],
  "exclude": [
    "**/node_modules",
    "test",
    "**/__mocks__",
    "**/*.spec.ts",
    "**/*.test.ts"
  ],
  "ts-node": {
    "transpileOnly": true,
    "experimentalSpecifierResolution": "node",
    "files": true,
    "esm": true,
    "swc": true
  }
}