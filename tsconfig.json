{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "target": "es2017",
    "sourceMap": true,
    "resolveJsonModule": true,
    "rootDir": ".",
    "baseUrl": "./",
    "paths": {
      "@nestjs-yalc/app": [
        "app/src"
      ],
      "@nestjs-yalc/app/*": [
        "app/src/*"
      ],
      "@nestjs-yalc/crud-gen": [
        "crud-gen/src"
      ],
      "@nestjs-yalc/crud-gen/*": [
        "crud-gen/src/*"
      ],
      "@nestjs-yalc/database": [
        "database/src"
      ],
      "@nestjs-yalc/database/*": [
        "database/src/*"
      ],
      "@nestjs-yalc/aws-helpers": [
        "aws-helpers/src"
      ],
      "@nestjs-yalc/aws-helpers/*": [
        "aws-helpers/src/*"
      ],
      "@nestjs-yalc/data-loader": [
        "data-loader/src"
      ],
      "@nestjs-yalc/data-loader/*": [
        "data-loader/src/*"
      ],
      "@nestjs-yalc/errors": [
        "errors/src"
      ],
      "@nestjs-yalc/errors/*": [
        "errors/src/*"
      ],
      "@nestjs-yalc/field-middleware": [
        "field-middleware/src"
      ],
      "@nestjs-yalc/field-middleware/*": [
        "field-middleware/src/*"
      ],
      "@nestjs-yalc/graphql": [
        "graphql/src"
      ],
      "@nestjs-yalc/graphql/*": [
        "graphql/src/*"
      ],
      "@nestjs-yalc/interfaces": [
        "interfaces/src"
      ],
      "@nestjs-yalc/interfaces/*": [
        "interfaces/src/*"
      ],
      "@nestjs-yalc/logger": [
        "logger/src"
      ],
      "@nestjs-yalc/logger/*": [
        "logger/src/*"
      ],
      "@nestjs-yalc/utils": [
        "utils/src"
      ],
      "@nestjs-yalc/utils/*": [
        "utils/src/*"
      ],
      "@nestjs-yalc/jest": [
        "jest/src"
      ],
      "@nestjs-yalc/jest/*": [
        "jest/src/*"
      ],
      "@nestjs-yalc/types": [
        "types/src"
      ],
      "@nestjs-yalc/types/*": [
        "types/src/*"
      ],
      "@nestjs-yalc/kafka": [
        "kafka/src"
      ],
      "@nestjs-yalc/kafka/*": [
        "kafka/src/*"
      ],
      "@nestjs-yalc/skeleton-module": [
        "examples/skeleton-module/src"
      ],
      "@nestjs-yalc/skeleton-module/*": [
        "examples/skeleton-module/src/*"
      ]
    }
  },
  "include": [
    "**/src/**/*.ts"
  ],
  "exclude": [
    "**/node_modules",
    "**/.webpack"
  ],
  "ts-node": {
    "transpileOnly": true,
    "files": true,
    "compilerOptions": {
      "esModuleInterop": false
    }
  }
}