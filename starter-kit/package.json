{
  "name": "skeleton-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "tsc --noEmit",
    "build:watch": "tsc-watch --noEmit",
    "lint": "npm run lint:no-fix -- --fix",
    "lint:no-fix": "eslint -c .eslintrc.cjs \"**/src/**/*.ts\" --max-warnings=0",
    "lint:path": "eslint -c .eslintrc.cjs $npm_config_lintpath\"/**/*.ts\" --max-warnings=0",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules --experimental-import-meta-resolve\" jest ",
    "test:watch": "npm run test -- --watch",
    "test:cov": "npm run test -- --coverage",
    "test:cov:serve": "npx http-server -c-1 ./docs/compodoc/jestcoverage/",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/sk-app/test/jest-e2e.json",
    "npm:update:onlylock": "npm update --package-lock-only",
    "npm:outdated": "npm outdated",
    "ncu:interactive": "npx -y npm-check-updates -i",
    "ncu": "npx -y npm-check-updates -u",
    "prebuild": "rimraf dist",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/sk-app/main"
  },
  "dependencies": {
    "@nestjs-yalc/api-strategy": "file:../api-strategy",
    "@nestjs-yalc/app": "file:../app",
    "@nestjs-yalc/crud-gen": "file:../crud-gen",
    "@nestjs-yalc/data-loader": "file:../data-loader",
    "@nestjs-yalc/database": "file:../database",
    "@nestjs-yalc/errors": "file:../errors",
    "@nestjs-yalc/field-middleware": "file:../field-middleware",
    "@nestjs-yalc/graphql": "file:../graphql",
    "@nestjs-yalc/interfaces": "file:../interfaces",
    "@nestjs-yalc/jest": "file:../jest",
    "@nestjs-yalc/logger": "file:../logger",
    "@nestjs-yalc/utils": "file:../utils",
    "@nestjs/common": "^9.2.1",
    "@nestjs/core": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
    "@types/nestjs-yalc": "file:../types",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.9",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.2.1",
    "@types/express": "^4.17.16",
    "@types/jest": "29.2.6",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.0",
    "prettier": "^2.8.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
